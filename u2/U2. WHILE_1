Proceso ACM
	//contraseña
	Escribir "Introduce la contraseña:"
	Leer contraseña
	
	Mientras contraseña <> "Acm256" Hacer
		Escribir "se llamara al supervisor:"
		Leer contraseña
	FinMientras
	
	Escribir "¡bienvenido!."
	
	
	suma <- 0	
	
	//piezas fabricadas
	escribir "-----piezas fabricadas--------"
	Escribir "Ingresa las pieza (0000 para salir):"
	Leer pieza
	
	Mientras pieza <> 0000 Hacer
		suma <- suma + pieza
		Escribir "Ingresa otro número (0000 para salir):"
		Leer pieza
		
	FinMientras
	
	Escribir "La suma total es: ", suma
	
	//if
	si  suma > 10000 entonces
		escribir "se ha cumplido la meta" 
	sino 
		escribir "alerta para produccion"
	FinSi
	
FinProceso

//resultado del codigo ejectado 
